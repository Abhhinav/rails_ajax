<p id="notice"><%= notice %></p>

<h1>Tasks</h1>

<br>

<%= link_to 'New Task', new_task_path %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Completed</th>
      <th>Position</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <%= render task%>
    <% end %>
  </tbody>
</table>


